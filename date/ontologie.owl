  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#areSpecializare">
    <rdfs:domain rdf:resource="#Antrenor"/>
    <rdfs:range rdf:resource="#Specializare"/>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="#esteProgramatLa">
    <rdfs:domain rdf:resource="#Client"/>
    <rdfs:range rdf:resource="#Programare"/>
  </owl:ObjectProperty>

